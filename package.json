{
	"name": "vlnn-tutorial",
	"displayName": "vlnn-tutorial",
	"description": "Simple commands to learn both VS Code extension development process and JS",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.infoMessage"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.infoMessage",
				"title": "Info from vlnn-tutorial"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
